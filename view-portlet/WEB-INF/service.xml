<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_1_0.dtd">
<service-builder package-path="com.test.slayer">
	<author>anshu</author>
	<namespace>abcProject</namespace>

	<entity name="iProject" local-service="true" remote-service="true" table="iproject_iproject">
<!-- PK fields -->
<column name="projectID" type="long" primary="true" />
<column name="projectName" type="String" primary="true" />
<!-- UI fields -->
<column name="skillset" type="String" />
<column name="description" type="String" />
<column name="contact" type="String" />
<!-- Audit fields -->
<column name="dateAdded" type="Date" />

	<!-- Order -->

		<order by="asc">
			<order-column name="projectName" />
		</order>

	</entity>
</service-builder>
