<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.test.slayer.model.iProject" />
	<class name="com.test.slayer.model.impl.iProjectImpl" table="iproject_iproject">
		<cache usage="read-write" />
		<composite-id name="primaryKey" class="com.test.slayer.service.persistence.iProjectPK">
			<key-property name="projectID" type="com.liferay.portal.dao.orm.hibernate.LongType" />
			<key-property name="projectName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		</composite-id>
		<property name="skillset" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="contact" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dateAdded" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>