<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.asu.poly.iproject.student.preferences.model.preferences" />
	<import class="com.asu.poly.iproject.student.preferences.model.Projectdetail" />
	<class name="com.asu.poly.iproject.student.preferences.model.impl.preferencesImpl" table="student_preferences">
		<cache usage="read-write" />
		<id name="studentID" type="long">
			<generator class="assigned" />
		</id>
		<property name="ASUID" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="preference1" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="preference2" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="preference3" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="preference4" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="preference5" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dateAdded" type="org.hibernate.type.TimestampType" />
		<property name="dateModified" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.asu.poly.iproject.student.preferences.model.impl.ProjectdetailImpl" table="iproject_projectdetail">
		<cache usage="read-write" />
		<id name="projectID" type="long">
			<generator class="assigned" />
		</id>
		<property name="projectTitle" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dateAdded" type="org.hibernate.type.TimestampType" />
		<property name="dateModified" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>