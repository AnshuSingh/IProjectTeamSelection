<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_1_0.dtd">

<service-builder package-path="com.asu.iproject.student.skillset">
	<author>zhengyan</author>
	<namespace>Skillset</namespace>
	<entity name="Skillset" local-service="true" remote-service="false">
		<!-- PK fields -->
		<column name="studentID" type="long" primary="true"></column>
		<!-- UI fields -->
		<column name="asuid" type="String"></column>
		<column name="email" type="String"></column>
		<column name="skill1" type="String"></column>
		<!-- Audit fields -->
		<column name="dateAdded" type="Date"></column>
	</entity>
</service-builder>