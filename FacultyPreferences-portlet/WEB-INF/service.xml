<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_1_0.dtd">

<service-builder package-path="com.asu.poly.iproject.faculty.preferences">
	<author>emente</author>
	<namespace>faculty</namespace>
	<entity name="Projectdetail" local-service="true" remote-service="true" table="iproject_projectdetail">
		<!-- PK fields -->
		<column name="projectID" type="long" primary="true"></column>
		<!-- UI fields -->
		<column name="projectTitle" type="String"></column>
		<!-- Audit fields -->
		<column name="dateAdded" type="Date"></column>
		<column name="dateModified" type="Date"></column>
	</entity>
	<entity name="preferences" local-service="true" remote-service="true">
		<!-- PK fields -->
		<column name="facultyID" type="long" primary="true"></column>
		<!-- UI fields -->
		<column name="ASUID" type="String"></column>
		<column name="preference1" type="String"></column>
		<column name="preference2" type="String"></column>
		<column name="preference3" type="String"></column>
		<!-- Audit fields -->
		<column name="dateAdded" type="Date"></column>
		<column name="dateModified" type="Date"></column>
	</entity>
</service-builder>