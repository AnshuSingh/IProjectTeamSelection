<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.subscriberapprove.model.impl.FacultyModelImpl">
		<attributes>
			<id name="facultyID" />
			<basic name="asuriteid" />
			<basic name="facultyName" />
			<basic name="facultyDept" />
			<basic name="contact" />
			<basic name="dateAdded">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="dateModified">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="assigned" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.subscriberapprove.model.impl.studentModelImpl">
		<attributes>
			<id name="studentgeninfoid" />
			<basic name="firstName" />
			<basic name="lastName" />
			<basic name="asuriteid" />
			<basic name="department" />
			<basic name="level" />
			<basic name="cgpa" />
			<basic name="sex" />
			<basic name="phone" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="assigned" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.subscriberapprove.model.impl.FacultyImpl" name="Faculty">
		<table name="Sapprove_Faculty" />
		<attributes />
	</entity>
	<entity class="com.subscriberapprove.model.impl.studentImpl" name="student">
		<table name="Sapprove_student" />
		<attributes />
	</entity>
</entity-mappings>